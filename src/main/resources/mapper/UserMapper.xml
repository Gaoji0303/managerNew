<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.managernew.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.managernew.pojo.User">
        <id column="id" property="id" />
        <result column="account" property="account" />
        <result column="password" property="password" />
        <result column="dept" property="dept" />
        <result column="email" property="email" />
        <result column="createTime" property="createTime" />
        <result column="updateTime" property="updateTime" />
        <result column="deleteTime" property="deleteTime" />
        <result column="deleteFlag" property="deleteFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, account, password, dept, email, createTime, updateTime, deleteTime, deleteFlag
    </sql>

</mapper>
